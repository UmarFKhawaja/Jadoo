{
  "name": "@jadoo/browser-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "watch:build": "vite build --watch",
    "watch:typecheck": "tsc --noEmit --watch",
    "watch": "concurrently \"npm:watch:typecheck\" \"npm:watch:typecheck\"",
    "clean": "rm -rf lib",
    "start": "dotenv -- vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.11.9",
    "@mantine/core": "^7.13.5",
    "@mantine/dates": "^7.13.5",
    "@mantine/form": "^7.13.5",
    "@mantine/hooks": "^7.13.5",
    "@mantine/modals": "^7.13.5",
    "@mantine/notifications": "^7.13.5",
    "@mantine/spotlight": "^7.13.5",
    "@tabler/icons-react": "^3.21.0",
    "chance": "^1.1.12",
    "dayjs": "^1.11.13",
    "jose": "^5.9.6",
    "nanoid": "^5.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.28.0",
    "react-router-dom": "^6.28.0",
    "swr": "^2.2.5",
    "typed-graphqlify": "^3.1.6"
  },
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "postcss": "^8.4.47",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
